<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model='ir.ui.view' id='product_kardex_form_view'>
        	<field name='name'>product.normal.send.kardex.form.view</field>
        	<field name='model'>product.product</field>
            <field name='inherit_id' ref='product.product_normal_form_view'/>
        	<field name='arch' type='xml'>
                <header>
                   <!-- <button name="print_kardex" string="Print Kardex" type="object"/> -->
                   <button name="print_report" string="Export" type="object" class="oe_highlight"/>
                </header>
                <xpath expr="//notebook/page[3]" position="after">
                    <page string="Report" name="report">
                        <group>
                            <field name='location_id'/>
                            <field name='fecha_inicial'/>
                            <field name='fecha_final'/>
                        </group>
                    </page>
                </xpath>
                
        	</field>
        </record>
    </data>
</openerp>